install:: atlas.xbo
	@if [ -d $(DESTDIR)$(LIBDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LIBDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) atlas.xbo $(DESTDIR)$(LIBDIR)/atlas.xbo

